{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://email-organizer-be.vercel.app/api/$1"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "https://email-organizer-be.vercel.app"
    }
  }
  